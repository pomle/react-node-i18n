{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "standard",
    "standard-react",
    "prettier",
    "eslint-config-prettier",
    "plugin:@typescript-eslint/eslint-recommended",
  ],
  "env": {
    "jest": true,
    "node": true,
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true,
    },
  },
  "settings": {
    "react": {
      "version": "16",
    },
  },
  "rules": {
    "space-before-function-paren": 0,
    "react/prop-types": 0,
    "react/jsx-handler-names": 0,
    "react/jsx-fragments": 0,
    "react/no-unused-prop-types": 0,

    "no-unused-vars": "off",
  },
}
